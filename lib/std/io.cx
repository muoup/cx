#include <stdio.h>

public:

void print(char* str) {
    for (char* ptr = str; *ptr; ptr += 1) {
        putchar(*ptr);
    }
}

void println(char* str) {
    puts(str);
}

void print_int(i32 i) {
    printf("%d", i);
}

void print_char(i32 c) {
    putchar(c);
}

private:

void printf(char* format, i32 i);