public:

int putchar(int c);

struct Inner {
    int a;
    int b;
}

struct Test {
    Inner inner;
    int c;
    int d;
}

void struct_parameter(Test t) {
    putchar(t.inner.a + '0');
    putchar(t.inner.b + '0');
    putchar(t.c + '0');
    putchar(t.d + '0');
    return;
}

Test struct_return() {
    return {
        .inner = {
            .a = 1,
            .b = 2
        },
        .c = 3,
        .d = 4
    };
}

int main() {
    Test t = struct_return();

    struct_parameter(t);
    return 0;
}