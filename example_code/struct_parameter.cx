public:

int putchar(int c);

struct Test {
    int a;
    int b;
    int c;
    int d;
}

void struct_parameter(Test t) {
    putchar(t.a + '0');
    putchar(t.b + '0');
    putchar(t.c + '0');
    putchar(t.d + '0');
    return;
}

Test struct_return() {
    Test t = {
        .a = 1,
        .b = 2,
        .c = 3,
        .d = 4
    };
    return t;
}

int main() {
    Test t = struct_return();

    struct_parameter(t);
    return 0;
}