public:

int putchar(int c);

struct Test {
    int a;
    int b;
    int c;
    int d;
}

void struct_parameter(Test t) {
    putchar(t.a + '0');
    return;
}

Test struct_return() {
    Test t = {1, 2, 3, 4};
    return t;
}

int main() {
    Test t = struct_return();

    struct_parameter(t);
    return 0;
}