void putchar(int str);

struct Point {
    int x;
    int y;
}

void Point::set(this, int x, int y) {
    this->x = x;
    this->y = y;
    return;
}

void Point::print(this) {
    putchar(this->x + '0');
    putchar(this->y + '0');
    return;
}

int main() {
    Point p;
    p.set(3, 4);
    p.print();
    return 0;
}