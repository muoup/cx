#include <stdlib.h>
#include <stdio.h>

int main(int argc, char** argv) {
    int x = atoi(argv[1]);

    switch (x) {
        case 1:
            printf("You entered one.\n");
            break;
        case 2:
            printf("You entered two.\n");
            break;
        case 3:
            printf("You entered three.\n");
            break;
        default:
            printf("You entered something else: %d\n", x);
    }
}