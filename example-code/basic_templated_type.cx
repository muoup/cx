template <T : type>
struct BasicTemplatedType {
    T value;
};

int main() {
    BasicTemplatedType<int> intInstance;
    intInstance.value = 42;
    BasicTemplatedType<float> floatInstance;
    floatInstance.value = 3.14;

    printf("Integer value: %d\n", intInstance.value);
    printf("Float value: %.2f\n", floatInstance.value);
}