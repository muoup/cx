#include <stdio.h>

int add(int x, int y) {
    return x + y;
}

int main() {
    int a = 10;
    int b = 5;
    int c = 2;

    int result1 = a + b * c;
    printf("Result 1: %d\n", result1);

    int result2 = (a + b) * c;
    printf("Result 2: %d\n", result2);

    int result3 = add(a, b) / c;
    printf("Result 3: %d\n", result3);

    int result4 = (a > b) && (c < b);
    printf("Result 4: %d\n", !!result4);

    return 0;
}
