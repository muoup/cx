#include <stdio.h>

struct Inner {
    int a;
    int b;
};

struct Test {
    Inner inner;
    int c;
    int d;
};

int main() {
    Test t = struct_return();

    pass_by_reference(&t);
    pass_by_value(t, 1);
    return 0;
}

void pass_by_reference(Test* t) {   
    printf("Struct Parameter:\n");
    printf("t.inner.a: %d\n", t.inner.a);
    printf("t.inner.b: %d\n", t.inner.b);
    printf("t.c: %d\n", t.c);
    printf("t.d: %d\n", t.d);
}

void pass_by_value(Test t, int c) {
    printf("Struct Parameter:\n");
    printf("t.inner.a: %d\n", t.inner.a);
    printf("t.inner.b: %d\n", t.inner.b);
    printf("t.c: %d\n", t.c);
    printf("t.d: %d\n", t.d);
}

Test struct_return() {
    return {
        .inner = {
            .a = 1,
            .b = 2
        },
        .c = 3,
        .d = 4
    };
}