#include <stdio.h>

struct MyStruct {
    int x;
    int y;
};

// Regular member function with 'this' parameter
void MyStruct::print(this) {
    printf("MyStruct: x=%d, y=%d\n", this->x, this->y);
}

// Static member function without 'this' parameter - acts as a constructor
MyStruct MyStruct::create(int x, int y) {
    printf("Creating MyStruct with x=%d, y=%d\n", x, y);
    return { .x = x, .y = y };
}

int main() {
    // Test static member function as constructor
    MyStruct s = MyStruct::create(5, 7);
    s.print();

    // Test regular member function
    MyStruct t;
    t.x = 10;
    t.y = 20;
    t.print();
    
    return 0;
}