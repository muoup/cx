#include <stdio.h>

int contract(int a, int b) 
where
    pre: (a > 0 && b > 0),
    // Note that signed overflow is UB in C, so this a sound postcondition
    post(result): (result > a && result > b)
{
    return a + b;
}

int main() {
    int x = 5;
    int y = 10;
    int z = contract(x, y);
    printf("Result: %d\n", z);
    return z;
}